<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    
    <!-- 默认背景色，会被代码中的setTint覆盖 -->
    <solid android:color="#757575" />
    
    <!-- 圆形边框 -->
    <stroke
        android:width="1dp"
        android:color="#FFFFFF" />
    
    <!-- 尺寸 -->
    <size
        android:width="48dp"
        android:height="48dp" />
</shape>
