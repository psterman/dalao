<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="#404CAF50" />
            <stroke android:width="1dp" android:color="#604CAF50" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval">
            <solid android:color="@color/dynamic_island_button_focused_background" />
            <stroke android:width="1dp" android:color="@color/dynamic_island_button_focused_stroke" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/dynamic_island_button_normal_background" />
            <stroke android:width="1dp" android:color="@color/dynamic_island_button_normal_stroke" />
        </shape>
    </item>
</selector>
